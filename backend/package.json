{
  "name": "yeeet-backend",
  "version": "1.0.0",
  "description": "Backend for Yeeet file upload service",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "init-db": "pnpm run generate && pnpm run migrate",
    "generate": "drizzle-kit generate",
    "migrate": "ts-node -r dotenv/config src/db/migrate.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/lib-storage": "^3.717.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@types/heic-convert": "^2.1.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.38.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "heic-convert": "^2.1.0",
    "mime": "^4.0.6",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.30.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
